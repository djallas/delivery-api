{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "RESTful API for Parcels Develivery APP",
  "main": "index.js",
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "report-dir": ".nyc_output",
    "cache": false,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "scripts": {
    "start": "nodemon  --require babel-register server.js --exit",
    "begin": "babel-node server.js --presets es2015,stage-2",
    "test": "nyc  --reporter=text --reporter=lcov mocha test/ --exit ",
    "test2": "nyc --reporter=text --reporter=lcov mocha --timeout 25000 --require babel-register test/ --exit",
    "test1": "nyc report --reporter=text --require babel-register mocha test/parcels.js --exit",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha test/",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint",
    "pg": "nodemon server.js"
  },
  "keywords": [
    "node",
    "restful",
    "api"
  ],
  "author": "Gilles Kagarama",
  "license": "ISC",
  "dependencies": {
    "joi": "^14.0.6",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "pg": "7.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "env": {
      "test": {
        "plugins": [
          "nyc"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cover": "^0.2.9",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "expect": "^23.6.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "lcov-parse": "^1.0.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.5",
    "nyc": "^13.1.0",
    "pg-pool": "^2.0.4",
    "supertest": "^3.3.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djallas/delivery-api.git"
  },
  "bugs": {
    "url": "https://github.com/djallas/delivery-api/issues"
  },
  "homepage": "https://github.com/djallas/delivery-api#readme"
}
